<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomBottomNavigationView">
        <attr format="color" name="backgroundTint"/>
        <attr format="dimension" name="customItemRadius"/>
        <attr format="dimension" name="customVerticalOffset"/>
        <attr format="dimension" name="customCornerRadius"/>
    </declare-styleable>
    <declare-styleable name="BottomSheetLayout">
        <attr name="collapsedHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FadingEdgeLayout">
        <attr name="fading_edge" format="integer">
            <flag name="top" value="1"/>
            <flag name="bottom" value="2"/>
            <flag name="left" value="4"/>
            <flag name="right" value="8"/>
        </attr>
        <attr name="fading_top" format="dimension"/>
        <attr name="fading_bottom" format="dimension"/>
        <attr name="fading_left" format="dimension"/>
        <attr name="fading_right" format="dimension"/>
    </declare-styleable>

</resources>